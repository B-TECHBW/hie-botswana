	"item": [
		{
			"name": "Add Patient To CR",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"",
							"pm.test(\"Successful POST request\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200]);",
							"});",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var moment = require('moment');",
							"",
							"pm.collectionVariables.set(\"patientMRN\", pm.variables.replaceIn('{{$randomUUID}}'));",
							"pm.collectionVariables.set(\"omang\", pm.variables.replaceIn('{{$randomUUID}}'));",
							"pm.collectionVariables.set(\"birthDate\", moment().subtract(_.random(1, 40), 'years').subtract(_.random(1, 150), 'days').format(\"YYYY-MM-DD\"));",
							"pm.collectionVariables.set(\"sex\", Math.random() < 0.5 ? \"male\" : \"female\");",
							"pm.collectionVariables.set(\"firstName\", pm.variables.replaceIn('{{$randomFirstName}}'));",
							"pm.collectionVariables.set(\"lastName\", pm.variables.replaceIn('{{$randomLastName}}'));",
							"pm.collectionVariables.set(\"facilityCode\", pm.variables.replaceIn('{{$randomUUID}}'));",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
							"value": "postman",
							"type": "string"
						},
						{
							"key": "username",
							"value": "postman",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"resourceType\" : \"Patient\",\n  \"id\" : \"{{patientMRN}}\",\n  \"meta\" : {\n    \"profile\" : [\n      \"http://b-techbw.github.io/bw-lab-ig/StructureDefinition/bw-patient\"\n    ]\n  },\n  \"identifier\" : [\n    {\n      \"system\" : \"http://moh.bw.org/ext/identifier/omang\",\n      \"value\" : \"{{omang}}\",\n      \"use\" : \"official\"\n    },\n    {\n        \"value\": \"urn:uuid:{{patientMRN}}\",\n        \"type\" : {\n        \"coding\" : [\n          {\n            \"code\" : \"MR\"\n          }\n        ]\n        },\n        \"system\" : \"urn:ietf:rfc:3986\"\n    }\n  ],\n  \"name\" : [\n    {\n      \"use\": \"official\",\n      \"family\": \"{{lastName}}\",\n      \"given\": [\n        \"{{firstName}}\"\n      ]\n    }\n  ],\n  \"gender\" : \"{{sex}}\",\n  \"birthDate\" : \"{{birthDate}}\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{openhim-url}}/CR/fhir/Patient",
					"host": [
						"{{openhim-url}}"
					],
					"path": [
						"CR",
						"fhir",
						"Patient"
					]
				}
			},
			"response": []
		},
		{
			"name": "Validate Patient in CR",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"    pm.response.to.be.withBody;",
							"    pm.response.to.be.json;",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"",
							"pm.test(\"A single patient record should be found.\", function () {",
							"    pm.expect(jsonData.total).to.eql(1);",
							"});",
							"",
							"pm.test(\"A golden record should be returned\", function() {",
							"    var entries = jsonData.entry;",
							"    pm.expect(entries).to.have.lengthOf(2);",
							"    pm.expect(entries[1].resource.meta.tag[0].display).to.eq(\"Golden Record\");",
							"});",
							"",
							"pm.test(\"Patient has correct birth date\", function() {",
							"    var patient = jsonData.entry[0].resource;",
							"    var dob = pm.collectionVariables.get(\"birthDate\");",
							"    pm.expect(patient.birthDate).to.eq(dob)",
							"});",
							"",
							"// Set Golden Record CRUID for Patient",
							"pm.collectionVariables.set(\"patientCRUID\", jsonData.entry[1].resource.id);",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "basic",
					"basic": [
						{
							"key": "password",
							"value": "postman",
							"type": "string"
						},
						{
							"key": "username",
							"value": "postman",
							"type": "string"
						}
					]
				},
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{openhim-url}}/CR/fhir/Patient?_id={{patientMRN}}&_include=Patient:link",
					"host": [
						"{{openhim-url}}"
					],
					"path": [
						"CR",
						"fhir",
						"Patient"
					],
					"query": [
						{
							"key": "_id",
							"value": "{{patientMRN}}"
						},
						{
							"key": "_include",
							"value": "Patient:link"
						}
					]
				}
			},
			"response": []
		},
